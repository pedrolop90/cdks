# Parámetros de la Lambda
LAMBDA_CODE_BUCKET=mi-bucket-lambda
LAMBDA_CODE_KEY=mi-lambda.zip
ID_SECURITY_GROUP=sg-xxxxxxxx
ID_CUENTA_AWS=123456789012
NOMBRE_LAMBDA=miLambda
RUNTIME=nodejs18.x
HANDLER=index.handler
TIMEOUT=30
MEMORY_SIZE=256
ACCESO_INTERNET=true

# Parámetros del API Gateway
API_GATEWAY_ID=abc123def
API_GATEWAY_PATH=/mi-recurso
API_GATEWAY_METHOD=POST
API_GATEWAY_PROXY=true

# Variables de entorno de la Lambda (Ejemplo)
VAR_ENTORNO_1=valor1
VAR_ENTORNO_2=valor2

# Permisos sobre otros recursos de AWS (Ejemplo)
PERMISO_1_NIVEL=logs:CreateLogGroup
PERMISO_1_ARN=arn:aws:logs:us-east-1:123456789012:log-group:/aws/lambda/miLambda
